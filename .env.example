# === AGI Alpha Node Bootstrap ===============================================
# Duplicate this file as `.env` and customise for your node. Any field can also
# be supplied via the environment when invoking the CLI or Docker image.
# The included keys are development defaults only – replace them before
# deploying to production.

# Core role selection: orchestrator | executor | validator | mixed
NODE_ROLE=mixed

# Human-readable identifier for this node (used to derive ENS name when one is
# not provided explicitly).
NODE_LABEL=demo-core

# Explicit ENS record (overrides NODE_LABEL + ENS_PARENT_DOMAIN)
NODE_ENS_NAME=demo.alpha.node.agi.eth
ENS_PARENT_DOMAIN=alpha.node.agi.eth

# Ethereum connectivity
RPC_URL=https://rpc.ankr.com/eth

# Operator identity & payout routes
OPERATOR_ADDRESS=0xd617363D45148Fae71190C64db3378D635E5c96B
NODE_PAYOUT_ETH_ADDRESS=0xd617363D45148Fae71190C64db3378D635E5c96B
NODE_PAYOUT_AGIALPHA_ADDRESS=0xd617363D45148Fae71190C64db3378D635E5c96B

# Signing keys (replace with secure keys before going live)
NODE_PRIVATE_KEY=0x59c6995e998f97a5a0044966f0945386f0b0c9cc1fa50bdbeeb29f44cbeb2c82
VALIDATOR_PRIVATE_KEY=0x8b3a350cf5c34c9194ca3abc0e3c870385b5f1984b8c7d1f6d7d3dbb2cd89f5c

# Telemetry & verifier endpoints
TELEMETRY_ENABLED=true
TELEMETRY_HASH_ALGO=sha256
METRICS_PORT=9464
ALPHA_NODE_OTEL_EXPORTER=console
ALPHA_NODE_OTLP_ENDPOINT=
ALPHA_NODE_OTEL_SAMPLING_RATIO=1.0
HEALTHCHECK_TIMEOUT=5000
VERIFIER_PORT=8787
VERIFIER_PUBLIC_BASE_URL=http://localhost:8787
API_PORT=8080
API_PUBLIC_READ_KEY=
API_DASHBOARD_ORIGIN=*
AGI_ALPHA_DB_PATH=:memory:

# Transport & NAT traversal controls
TRANSPORT_ENABLE_QUIC=true
TRANSPORT_ENABLE_TCP=true
ENABLE_HOLE_PUNCHING=true
AUTONAT_ENABLED=true
AUTONAT_THROTTLE_SECONDS=60
RELAY_ENABLE_CLIENT=true
RELAY_ENABLE_SERVER=false
RELAY_MAX_RESERVATIONS=32
RELAY_MAX_CIRCUITS_PER_PEER=8
# Leave blank to disable enforcement; set to an integer bits-per-second ceiling for relayed streams
RELAY_MAX_BANDWIDTH_BPS=

# Job registry profile & governance defaults
JOB_REGISTRY_PROFILE=v0
JOB_APPLICATION_PROOF=0x

# Validator quorum expectations for demos (adjust to match mainnet policy)
VALIDATION_QUORUM_BPS=6667
VALIDATION_MINIMUM_VOTES=1

# Validator runtime & journaling behaviour
VALIDATOR_SOURCE_TYPE=memory
VALIDATOR_SOURCE_PATH=
VALIDATOR_SINK_TYPE=memory
LIFECYCLE_LOG_DIR=.agi/lifecycle

# On-chain registry overrides (leave blank to auto-discover)
JOB_REGISTRY_ADDRESS=
IDENTITY_REGISTRY_ADDRESS=
STAKE_MANAGER_ADDRESS=
REWARD_ENGINE_ADDRESS=
DESIRED_JOB_REGISTRY_ADDRESS=
DESIRED_IDENTITY_REGISTRY_ADDRESS=
DESIRED_VALIDATION_MODULE_ADDRESS=
DESIRED_REPUTATION_MODULE_ADDRESS=
DESIRED_DISPUTE_MODULE_ADDRESS=

# Operator & validator share overrides
AUTO_STAKE=false
STAKE_AMOUNT=
AUTO_RESUME=false
DESIRED_MINIMUM_STAKE=
DESIRED_OPERATOR_SHARE_BPS=
DESIRED_VALIDATOR_SHARE_BPS=
DESIRED_TREASURY_SHARE_BPS=
ROLE_SHARE_TARGETS=
SYSTEM_PAUSE_ADDRESS=
PLATFORM_INCENTIVES_ADDRESS=

# Governance ledger exports
GOVERNANCE_API_TOKEN=
GOVERNANCE_LEDGER_ROOT=

# Health gate & observability controls
HEALTH_GATE_ALLOWLIST=*.agent.agi.eth,*.alpha.agent.agi.eth,*.node.agi.eth,*.alpha.node.agi.eth,*.alpha.club.agi.eth,*.club.agi.eth
HEALTH_GATE_INITIAL_STATE=false
HEALTH_GATE_OVERRIDE_ENS=

# Offline execution & snapshots
OFFLINE_MODE=false
OFFLINE_SNAPSHOT_PATH=

# Custom work-unit weighting (JSON object; leave blank for defaults)
WORK_UNITS=

# α-WU token binding (do not change – canonical contract on mainnet)
AGIALPHA_TOKEN_ADDRESS=0xa61a3b3a130a9c20768eebf97e21515a6046a1fa
AGIALPHA_TOKEN_DECIMALS=18

# ENS network overrides (optional; leave blank to use built-in presets)
ALPHA_NODE_CHAIN_ID=
ALPHA_NODE_RPC_URL=
ALPHA_NODE_ENS_REGISTRY=
ALPHA_NODE_NAME_WRAPPER=
ALPHA_NODE_PUBLIC_RESOLVER=
