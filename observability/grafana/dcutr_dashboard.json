{
  "__inputs": [],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "11.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "2.51.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "11.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "11.0.0" },
    { "type": "panel", "id": "heatmap", "name": "Heatmap", "version": "11.0.0" }
  ],
  "title": "DCUtR — Hole Punch Performance",
  "uid": "dcutr-observability",
  "version": 1,
  "time": { "from": "now-6h", "to": "now" },
  "timezone": "browser",
  "tags": ["dcutr", "libp2p", "observability", "hole-punch"],
  "schemaVersion": 39,
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Punch Success Rate",
      "datasource": { "type": "prometheus", "uid": "__PROMETHEUS__" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(dcutr_punch_success_total[5m])) / sum(rate(dcutr_punch_attempts_total[5m]))",
          "legendFormat": "punch success %"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "vertical"
      },
      "fieldConfig": {
        "defaults": { "unit": "percentunit", "min": 0, "max": 1 },
        "overrides": []
      },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Punch Attempts / Success / Failures",
      "datasource": { "type": "prometheus", "uid": "__PROMETHEUS__" },
      "targets": [
        { "refId": "A", "expr": "sum(rate(dcutr_punch_attempts_total[5m]))", "legendFormat": "attempts" },
        { "refId": "B", "expr": "sum(rate(dcutr_punch_success_total[5m]))", "legendFormat": "success" },
        { "refId": "C", "expr": "sum(rate(dcutr_punch_failure_total[5m]))", "legendFormat": "failures" }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 6, "y": 0 }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Time To Direct (Latency Histogram)",
      "datasource": { "type": "prometheus", "uid": "__PROMETHEUS__" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(dcutr_time_to_direct_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Relay Offload",
      "datasource": { "type": "prometheus", "uid": "__PROMETHEUS__" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(dcutr_relay_offload_total[5m]))",
          "legendFormat": "offload"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Direct Path Quality (RTT)",
      "datasource": { "type": "prometheus", "uid": "__PROMETHEUS__" },
      "targets": [
        { "refId": "A", "expr": "avg(dcutr_path_quality_rtt_ms)", "legendFormat": "avg rtt" }
      ],
      "fieldConfig": { "defaults": { "unit": "ms" }, "overrides": [] },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 }
    },
    {
      "id": 6,
      "type": "heatmap",
      "title": "Region × ASN Heatmap",
      "datasource": { "type": "prometheus", "uid": "__PROMETHEUS__" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(dcutr_punch_success_total[10m])) by (region, asn)",
          "legendFormat": "{{region}} / {{asn}}"
        }
      ],
      "options": {
        "color": { "mode": "spectrum" },
        "heatmap": {}
      },
      "fieldConfig": {
        "defaults": { "unit": "short" },
        "overrides": []
      },
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 14 }
    }
  ],
  "templating": { "list": [] },
  "annotations": { "list": [] },
  "links": [
    {
      "title": "DCUtR METRICS.md",
      "url": "https://github.com/MontrealAI/AGI-Alpha-Node-v0/blob/main/observability/docs/METRICS.md",
      "type": "link"
    },
    {
      "title": "DCUtR Dashboard Guide",
      "url": "https://github.com/MontrealAI/AGI-Alpha-Node-v0/blob/main/observability/docs/DASHBOARD.md",
      "type": "link"
    }
  ]
}
